{
    "name": "authentication",
    "version": "2.0.0",
    "description": "Authentication module for toy-exchange web app.",
    "main": "dist/server.js",
    "types": "dist/server.d.ts",
    "scripts": {
        "start": "sh commands/start.sh",
        "start:dev": "sh commands/start.dev.sh",
        "start:test": "sh commands/start.test.sh",
        "start:docker": "cd ../../../deployment && docker-compose -f docker-compose.yaml up --build authentication",
        "start:dev:docker": "cd ../../../deployment && docker-compose -f docker-compose.dev.yaml up --build authentication-dev",
        "start:test:docker": "cd ../../../deployment && docker-compose -f docker-compose.test.yaml up --build authentication-test",
        "serve": "sh commands/serve.sh",
        "build": "sh commands/build.sh",
        "build:interfaces": "sh commands/build.interfaces.sh",
        "build:selective": "sh commands/build.selective.sh",
        "build:commands": "sh commands/build.selective.sh commands",
        "publish:interfaces": "sh commands/publish.interfaces.sh",
        "clean": "sh commands/clean.sh",
        "install:all": "sh commands/install.sh",
        "_install:non-local-deps": "sh commands/install.non-local-deps.sh",
        "_install:local-deps": "sh commands/install.local-deps.sh",
        "test": "echo \"Error: no test specified\" && exit 1",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "regular-user:gen-jwt": "sh regular-user/commands/gen-jwt.sh",
        "regular-user:publish-interfaces": "sh regular-user/commands/publish-interfaces.sh",
        "regular-user:publish-interfaces:self": "sh regular-user/commands/publish-interfaces.self.sh",
        "regular-user:build:commands": "sh regular-user/commands/build.selective.sh commands"
    },
    "author": "Aleksandar Radenkovic (Sale)",
    "license": "UNLICENSED",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AREA4-Brunch/toy-exchange.git"
    },
    "bugs": {
        "url": "https://github.com/AREA4-Brunch/toy-exchange/issues"
    },
    "homepage": "https://github.com/AREA4-Brunch/toy-exchange#readme",
    "devDependencies": {
        "@types/cors": "^2.8.18",
        "@types/express": "^5.0.1",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/node": "^22.15.17",
        "prettier": "^3.5.3"
    },
    "dependencies": {
        "authentication-interfaces": "file:shared/libs/authentication-interfaces-2.0.0.tgz",
        "authorization": "file:shared/libs/authorization-1.3.1.tgz",
        "common": "file:shared/libs/common-2.2.3.tgz",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "jsonwebtoken": "^9.0.2",
        "password-utils": "file:regular-user/login/libs/password-utils-1.0.1.tgz",
        "reflect-metadata": "^0.2.2",
        "tsyringe": "^4.10.0",
        "xss": "^1.0.15",
        "zod": "^3.24.4",
        "typescript": "^5.8.3"
    },
    "files": [
        "dist/"
    ]
}
